  rdfs:label "description"@en ;
  rdfs:comment "Description of the name authority." ;
  cims:multiplicity cims:M:0..1 ;
  cims:stereotype "NC", uml:attribute ;
  rdfs:domain nc:NamingAuthority ;
  rdfs:range xsd:string .

