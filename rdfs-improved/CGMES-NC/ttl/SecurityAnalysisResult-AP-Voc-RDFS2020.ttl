  cims:belongsToCategory sar:Package_SecurityAnalysisResultProfile ;
  cims:stereotype "NC", uml:concrete .

nc:Stage a owl:Class ;
  rdfs:label "Stage"@en ;
  rdfs:comment "Stage of a remedial action scheme." ;
  cims:belongsToCategory sar:Package_SecurityAnalysisResultProfile ;
  cims:stereotype "NC", uml:concrete .

