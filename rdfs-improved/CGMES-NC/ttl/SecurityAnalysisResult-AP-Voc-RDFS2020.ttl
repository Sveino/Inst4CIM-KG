  cims:multiplicity cims:M:0..n ;
  cims:stereotype "NC" ;
  owl:inverseOf nc:RemedialActionApplied.RemedialAction ;
  rdfs:domain nc:RemedialAction ;
  rdfs:range nc:RemedialActionApplied .

