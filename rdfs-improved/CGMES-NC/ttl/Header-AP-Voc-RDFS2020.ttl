  rdfs:domain md:Model ;
  rdfs:range md:Model .

dcterms:Resource13 a rdf:Property ;
  rdfs:label "Resource13"@en ;
  rdfs:comment "The resource that has these recerences." ;
  cims:AssociationUsed "No" ;
  cims:inverseRoleName dcterms:references ;
