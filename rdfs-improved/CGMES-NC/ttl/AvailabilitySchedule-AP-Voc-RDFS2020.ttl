  cims:stereotype "NC", uml:concrete .

nc:GridStateAlteration a owl:Class ;
  rdfs:label "GridStateAlteration" ;
  rdfs:comment "Grid state alteration is a change of values describing state (operating point) of one element in the grid model compared to the base case."@en ;
