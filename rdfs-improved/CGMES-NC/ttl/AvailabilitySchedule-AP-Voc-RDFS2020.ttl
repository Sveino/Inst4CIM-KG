  cims:stereotype "NC" .

nc:TimeSeriesInterpolationKind a owl:Class ;
  rdfs:label "TimeSeriesInterpolationKind" ;
  rdfs:comment "Kinds of interpolation of values between two time point."@en ;
  cims:belongsToCategory as:Package_AvailabilityScheduleProfile ;
  cims:stereotype "NC", uml:enumeration .

