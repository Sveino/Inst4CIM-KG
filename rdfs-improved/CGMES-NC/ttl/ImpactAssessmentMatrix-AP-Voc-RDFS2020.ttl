  cims:multiplicity cims:M:1 ;
  cims:stereotype "NC" ;
  rdfs:domain nc:RemedialActionOutcomeValue ;
  rdfs:range nc:RemedialAction .

nc:RemedialActionOutcomeValue.impactQuantity a rdf:Property ;
  rdfs:label "impactQuantity"@en ;
