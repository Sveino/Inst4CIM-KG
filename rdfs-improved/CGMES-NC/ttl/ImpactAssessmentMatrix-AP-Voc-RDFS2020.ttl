iam:Ontology a owl:Ontology ;
  dct:description "This vocabulary is describing the impact assessment matrix profile."@en ;
  dcat:keyword "IAM" ;
  dcat:theme "vocabulary"@en ;
  dct:conformsTo "file://CIM100_CGMES31v01_501-20v02_NC23v62_MM10v01.eap", "file://iec61970cim17v40_iec61968cim13v13a_iec62325cim03v17a.eap", "urn:iso:std:iec:61970-301:ed-7:amd1", "urn:iso:std:iec:61970-401:draft:ed-1", "urn:iso:std:iec:61970-501:draft:ed-2", "urn:iso:std:iec:61970-600-2:ed-1" ;
  dct:creator "ENTSO-E CIM WG NC project"@en ;
