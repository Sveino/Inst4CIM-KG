  rdfs:range nc:OutcomeValue .

nc:OutcomeValue.ImpactAssessmentMatrix a rdf:Property ;
  rdfs:label "ImpactAssessmentMatrix"@en ;
  rdfs:comment "the impact assessment matrix which has this value." ;
