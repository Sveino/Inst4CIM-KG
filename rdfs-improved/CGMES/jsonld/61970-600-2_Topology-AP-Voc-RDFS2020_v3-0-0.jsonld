      "@id": "cim:ACDCConverterDCTerminal",
      "@type": "owl:Class",
      "cims:belongsToCategory": "tp:Package_TopologyProfile",
      "cims:stereotype": [
        "Description",
        {
          "@id": "uml:concrete"
        }
