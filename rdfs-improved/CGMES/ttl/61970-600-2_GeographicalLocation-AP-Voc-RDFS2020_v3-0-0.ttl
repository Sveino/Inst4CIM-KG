  cims:multiplicity cims:M:0..1 ;
  cims:stereotype uml:attribute ;
  rdfs:domain cim:Status ;
  rdfs:range xsd:dateTime .

cim:Status.reason a owl:DatatypeProperty, owl:FunctionalProperty ;
  rdfs:label "reason" ;
