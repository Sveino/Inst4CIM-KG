  rdfs:domain eu:IdentifiedObject ;
  rdfs:range xsd:string ;
  skos:definition "The attribute is used for an exchange of a human readable short name with length of the string 12 characters maximum."@en ;
  tp:isEuropean "True" ;
  tp:isdeprecated "True" .

