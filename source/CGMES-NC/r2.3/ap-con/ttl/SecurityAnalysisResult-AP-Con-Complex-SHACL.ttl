		sh:message      "PowerFlowResult.valueVAR is not provided for ReactivePowerLimit." ;
		sh:prefixes cim: ;
		sh:select """
			SELECT  $this  
			WHERE {      
        $this nc:PowerFlowResult.OperationalLimit/rdf:type ?oltype.
