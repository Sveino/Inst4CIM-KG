  <rdf:Description rdf:about="https://cim4.eu/ns/nc#RemedialActionSchedule">
    <cims:belongsToCategory rdf:resource="https://ap.cim4.eu/ImpactAssessmentMatrix#Package_ImpactAssessmentMatrixProfile"/>
    <rdfs:comment>A schedule for a determined remedial action.</rdfs:comment>
