  <rdf:Description rdf:about="https://cim4.eu/ns/nc#QualitativeRemedialActionImpact">
    <cims:belongsToCategory rdf:resource="https://ap.cim4.eu/ImpactAssessmentMatrix#Package_ImpactAssessmentMatrixProfile"/>
    <rdfs:comment>Defines the qualitative impact for a remedial action. Relevant remedial action is assumed to have impact when the impact quantity is applied.</rdfs:comment>
    <rdfs:label xml:lang="en">QualitativeRemedialActionImpact</rdfs:label>
    <cims:stereotype>NC</cims:stereotype>
    <cims:stereotype rdf:resource="http://iec.ch/TC57/NonStandard/UML#concrete"/>
    <rdfs:subClassOf rdf:resource="https://cim4.eu/ns/nc#RemedialActionImpact"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </rdf:Description>
