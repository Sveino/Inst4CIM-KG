  </rdf:Description>
  <rdf:Description rdf:about="https://cim4.eu/ns/nc#OutcomeValue">
    <cims:belongsToCategory rdf:resource="https://ap.cim4.eu/ImpactAssessmentMatrix#Package_ImpactAssessmentMatrixProfile"/>
    <rdfs:comment>Outcome of an impact assessment matrix.</rdfs:comment>
    <rdfs:label xml:lang="en">OutcomeValue</rdfs:label>
    <cims:stereotype>NC</cims:stereotype>
