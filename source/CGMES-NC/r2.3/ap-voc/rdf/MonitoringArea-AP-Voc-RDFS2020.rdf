  <rdf:Description rdf:about="https://cim4.eu/ns/nc#AreaBorderTerminal">
    <cims:belongsToCategory rdf:resource="https://ap-voc.cim4.eu/MonitoringArea#Package_MonitoringAreaProfile"/>
