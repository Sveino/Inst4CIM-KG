    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://cim4.eu/ns/nc#AreaBorderTerminal.MonitoringArea">
    <cims:AssociationUsed>Yes</cims:AssociationUsed>
